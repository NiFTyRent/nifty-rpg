{
  "name": "phaser3-project-template",
  "version": "1.1.0",
  "description": "A Phaser 3 Project Template",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --config webpack/prod.js ",
    "start": "webpack-dev-server --config webpack/base.js --open",
    "jsdoc": "jsdoc src -r -c jsdoc.config.json && npx browser-sync start --server --files 'docs/*.html' --startPath docs/ --watch 'docs/*.html'",
    "distwatch": "npx browser-sync start --server 'dist'  --watch 'disc/*.html'",
    "publish-git": "npm run build && cd dist && git init && git remote add pages https://github.com/SkyAlpha/luminus-rpg.github.io && git add -A . && git commit -m \"$npm_config_message\" && git push pages master --force && cd .. "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/photonstorm/phaser3-project-template.git"
  },
  "author": "Richard Davey <rdavey@gmail.com> (http://www.photonstorm.com)",
  "license": "MIT",
  "licenseUrl": "http://www.opensource.org/licenses/mit-license.php",
  "bugs": {
    "url": "https://github.com/photonstorm/phaser3-project-template/issues"
  },
  "homepage": "https://github.com/photonstorm/phaser3-project-template#readme",
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/preset-env": "^7.7.1",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.13",
    "clean-webpack-plugin": "^3.0.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.6.6",
    "raw-loader": "^3.1.0",
    "terser-webpack-plugin": "^2.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@mikewesthad/dungeon": "^2.0.1",
    "css-loader": "^5.0.1",
    "ink-docstrap": "^1.3.2",
    "nes.css": "^2.3.0",
    "phaser": "^3.50.0",
    "phaser3-juice-plugin": "git+https://github.com/RetroVX/phaser3-juice-plugin.git",
    "phaser3-nineslice": "^0.5.0",
    "phaser3-rex-plugins": "^1.1.31"
  }
}
